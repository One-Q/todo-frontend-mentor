{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "max-len": 0,
    "no-underscore-dangle": 0,
    "react/prop-types": "warn"
  },
  "globals": {
    "jest": true,
    "expect": true,
    "before": true,
    "after": true,
    "it": true,
    "describe": true,
    "beforeEach": true,
    "afterEach": true
  }
}